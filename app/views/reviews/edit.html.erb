<div class="col-md-6 col-md-offset-3"> 
  <div class="edit-review-form">

<h1>Edit Review</h1>

<%= simple_form_for([@company, @review]) do |r| %>
<div id ="rating-form">
  <label>Rating</label>
</div>
<%= r.input :comment %>
<%= r.button :submit, :class => "btn-custom2" %>
<% end %>
</div>
</div>
    
    
<script>
  $('#rating-form').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
  });
</script>
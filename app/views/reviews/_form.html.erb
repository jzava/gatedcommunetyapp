<%= simple_form_for([@company, @company.reviews.build]) do |r| %>
<div id ="rating-form">
  <label>Rating</label>
</div>
<%= r.input :comment %>
<%= r.button :submit, :class => "btn-custom2" %>
<% end %>

<script type='text/javascript' src='http://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js?ver=1.4.2'>
  $('#rating-form').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
  });
</script>

